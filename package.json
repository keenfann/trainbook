{
  "name": "trainbook",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\" \"npm run dev:open\"",
    "dev:web": "vite",
    "dev:server": "node --watch server/index.js",
    "dev:open": "node -e \"if(process.env.CI==='true'||process.env.TRAINBOOK_SKIP_OPEN==='true'){process.exit(0);}const port=process.env.VITE_PORT||'5173';const url='http://localhost:'+port;const {exec}=require('child_process');const cmds=[];if(process.platform==='darwin'){cmds.push('open -a \\\"Google Chrome\\\" '+url,'open '+url);}else if(process.platform==='win32'){cmds.push('start \\\"\\\" '+url);}else{cmds.push('xdg-open '+url);}const cmd=cmds.find(Boolean);setTimeout(()=>{if(cmd){exec(cmd);}},2000);\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "sync:exercise-library": "node server/scripts/sync-exercisedb-library.mjs"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "express": "5.2.1",
    "express-session": "^1.18.0",
    "framer-motion": "^12.34.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0"
  },
  "overrides": {
    "router": "2.2.0",
    "path-to-regexp": "8.3.0"
  },
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "jsdom": "^28.0.0",
    "supertest": "^7.2.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
